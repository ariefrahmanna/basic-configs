{
  "arrowParens": "always",
  "endOfLine": "lf",
  "importOrder": ["^react$", "^(?!^react$|^@/|^[./]).*", "^@/(.*)$", "^[./]"],
  "importOrderParserPlugins": ["jsx", "typescript"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  // "plugins": [
  //   "@trivago/prettier-plugin-sort-imports",
  //   "prettier-plugin-tailwindcss"
  // ],
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "es5"
}
